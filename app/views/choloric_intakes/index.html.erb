<p id = "notice"><%= notice %></p>
<h1>Calorie Intake</h1>

<table>
  <thead>
    <tr>
      <th>Calories</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @choloric_intake.each do |choloric_intake| %>
      <tr>
        <td><%= choloric_intake.calories %></td>
        <td><%= choloric_intake.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Enter Calories Consumed', new_choloric_intakes_post_path %>
<%= link_to "Back to the Homepage", root_path %>
